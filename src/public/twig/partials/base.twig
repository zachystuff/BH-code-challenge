<!DOCTYPE html>
<html>
	<head>
		{% block script %}
			<script src="javascript/everywhere.js"></script>
		{% endblock %}
		{% block css %}
			<link rel="stylesheet" href="css/everywhere.css">
		{% endblock %}
		{% block head %}
			<title>
				{% block title %}{% endblock %}
			</title>
		{% endblock %}
		{% block customscripts %}{% endblock %}
	</head>
	<body>
		{% block header %}
			{% include 'partials/header.twig' %}
		{% endblock %}
		<div class='container'>
			{% block content %}{% endblock %}
		</div>
		{% block footer %}
			{% include 'partials/footer.twig' %}
		{% endblock %}
	</body>
</html>
